{
    "name": "meu-projeto/laravel",
    "description": "Projeto Laravel em Docker",
    "require": {
      "php": "^8.2",
      "laravel/framework": "^10.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/"
      }
    },
    "scripts": {
      "post-install-cmd": [
        "php artisan cache:clear"
      ]
    },
    "config": {
      "optimize-autoloader": true
    }
  }
  